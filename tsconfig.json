{
  "compilerOptions": {
    "target": "es2016",
    "lib": [ "ES6" ], 
    "module": "esnext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": true,
    "moduleResolution": "node",
  },
  "include": ["**/*.ts"],
  "exclude": ["index.js"],
}
